+            ax[j].axis("off")
+            ax[j] = fig.add_axes([0, 0.05, 1, 0.9], fc=[0, 0, 0, 0])
+
+    ax = axes.ravel()[-1]
+    for axis in ["left", "right"]:
+        ax.spines[axis].set_visible(False)
+    ax.set_xticks([])
+    ax.set_yticks([])
